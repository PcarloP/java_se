package day_15;

public class Matrix {

	public static void main(String[] args) {

		int[][] matrix = { {9,8,7} , //[0] --> [9,8,7]
				           {5,3,2} , //[1] --> [5,3,2]
				           {6,6,7}   //[2] --> [6,6,7]
				         };          //indici	0,1,2 
		
		for(int i = 0 ;i < matrix[0].length ; i++) {
			
			System.out.print(matrix[0][i]);
			
		}
		
		
		
		
		

		
		
		
		
		
		
		
		
		
	}

}
