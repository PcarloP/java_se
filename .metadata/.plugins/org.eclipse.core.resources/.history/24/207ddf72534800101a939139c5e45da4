package day_17.JDBC.persistence;

public class DAOExeption extends Exception {


		public DAOExeption() {
		super();
		}
		public DAOExeption (String message, Throwable cause) {
		super(message, cause);
		}
		public DAOExeption (String message) {
		super(message);
		}
		public DAOExeption (Throwable cause) {
		super(cause);
		}

		
	
	
}
