package day_16;

import java.math.BigDecimal;

public class ClientiLuce extends Cliente {

	private double kwh;//quantita di consumo wkh
	private BigDecimal Kwh;//prezzo da pagare in base al consumo
	
	public ClientiLuce() {
		super();
	}
	public ClientiLuce(String cf, String nome, String cognome,) {
		super();
	}
	public double getKwh() {
		return kwh;
	}
	public void setKwh(double kwh) {
		this.kwh = kwh;
	}
	public BigDecimal getKwh() {
		return Kwh;
	}
	public void setKwh(BigDecimal kwh) {
		Kwh = kwh;
	}
	@Override
	public String toString() {
		return "ClientiLuce [kwh=" + kwh + ", Kwh=" + Kwh + "]";
	}
	

	
	
	
	
	
	
	
	
	

}
